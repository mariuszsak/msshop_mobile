    "axios": "^0.21.1",
    "expo": "~40.0.0",
    "expo-font": "8.4.0",
    "expo-status-bar": "~1.0.3",
    "firebase": "^8.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-dotenv": "^2.5.5",
